<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="DataDemo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:DataDemo"
    Shell.FlyoutBehavior="Locked"
    Title="DataDemo">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Campuses"
        ContentTemplate="{DataTemplate local:Pages.CampusesPage}"
        Route="CampusesPage" />

</Shell>
